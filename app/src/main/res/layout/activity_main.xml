<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_home_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainhome_bg"

    tools:context=".main_act">

    <RelativeLayout

        android:id="@+id/home_ch_mot"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="256dp"
        android:background="@drawable/home_ch_mot">

        <RelativeLayout

            android:id="@+id/home_ch_touc"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="90dp"
            android:layout_marginTop="90dp"

            android:background="@drawable/home_ch_touc">

            <View
                android:id="@+id/main_earth2"
                android:layout_width="117dp"
                android:layout_height="87dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="1dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/main_earth2" />


        </RelativeLayout>


        <ImageButton
            android:id="@+id/mainhome_ch"
            android:layout_width="218dp"
            android:layout_height="231dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="41dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/mainhome_ch2" />

    </RelativeLayout>


    <!-- mainHome_slogon -->
    <RelativeLayout


        android:id="@+id/mainhome_sl"
        android:layout_width="323dp"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="84dp"

        android:background="@drawable/mainhome_sl">

        <TextView
            android:id="@+id/text_mainho"
            android:layout_width="343dp"
            android:layout_height="65dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:lineSpacingExtra="0sp"
            android:text="@string/text_mainho"
            android:textAppearance="@style/text_mainho"


            />
    </RelativeLayout>

    <ImageButton
        android:id="@+id/mainhome_start_button1"
        android:layout_width="109dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="603dp"
        android:background="@drawable/mainhome_start_button"
        android:contentDescription="TODO" />


</RelativeLayout>
